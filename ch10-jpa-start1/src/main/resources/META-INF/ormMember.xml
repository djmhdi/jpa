<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" version="2.1">
	<named-query name="xml.CH10_OOQ_MEMBER.findByUserName">
		<query><![CDATA[
			SELECT m
			FROM CH10_OOQ_MEMBER m
			WHERE m.userName = :userName
		]]></query>
	</named-query>
	
	<named-query name="xml.CH10_OOQ_MEMBER.count">
		<query><![CDATA[
			SELECT COUNT(m)
			FROM CH10_OOQ_MEMBER m
		]]></query>
	</named-query>
</entity-mappings>